{
  "name": "retail-web-usa",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "ng build",
    "e2e": "ng e2e --configuration=test",
    "format": "prettier --write \"{apps,libs}/**/*.ts\"",
    "format:check": "prettier --list-different \"{apps,libs}/**/*.ts\"",
    "lint": "ng lint",
    "ng": "ng",
    "package:apps": "bb-ang build-apps --output-hashing --serial-build --build-memory=8192",
    "postpackage:apps": "bb-ang create-provisioning-package",
    "package:libs": "bb-ang package-libs",
    "prestart": "bb-ang copy-assets",
    "start": "ng serve",
    "test": "ng test",
    "update": "ng update @bb-cli/schematics@^4 @backbase/foundation-ang@^6 @backbase/banking-schematics^1",
    "xi18n:retail": "ng xi18n retail-app --output-path=src/locale && xliffmerge --profile apps/retail-app/src/xliffmerge.json en es",
    "create-experience:retail": "sdlc package apps/retail-app/sdlc/experience.json",
    "bundle-home-page:retail": "sdlc convert-from apps/retail-app/sdlc/experience.json apps/retail-app/sdlc/pages/home.json -l silent --remove-defaults false -o dist/apps/retail-app/page-model-mock.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.0",
    "@angular/cdk": "~10.1.0",
    "@angular/common": "~10.1.0",
    "@angular/compiler": "~10.1.0",
    "@angular/core": "~10.1.0",
    "@angular/forms": "~10.1.0",
    "@angular/platform-browser": "~10.1.0",
    "@angular/platform-browser-dynamic": "~10.1.0",
    "@angular/router": "~10.1.0",
    "@backbase/actions-mocks-provider-ang": "3.0.1",
    "@backbase/banking-schematics": "^1.0.0-beta.133",
    "@backbase/data-ang": "3.9.0",
    "@backbase/foundation-ang": "6.7.8",
    "@backbase/identity-ang": "3.9.0",
    "@backbase/notifications-mocks-provider-ang": "3.0.3",
    "@backbase/retail-ang": "3.9.0",
    "@backbase/ui-ang": "5.28.5",
    "@backbase/universal-ang": "6.9.3",
    "@ng-bootstrap/ng-bootstrap": "~7.0.0",
    "@ng-select/ng-select": "~5.0.0",
    "@ngrx/effects": "~10.0.0",
    "@ngrx/store": "~10.0.0",
    "@swimlane/ngx-charts": "~16.0.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.6.5",
